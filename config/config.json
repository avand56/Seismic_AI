{
    "models": [
      {
        "name": "UNet_2D",
        "input_shape": [256, 256, 1],
        "num_classes": 3,
        "dimensionality": 2,
        "optimizer": "adam",
        "loss": "categorical_crossentropy",
        "metrics": ["accuracy"],
        "epochs": 20,
        "batch_size": 32
      },
      {
        "name": "DeepLSTM",
        "input_shape": [100, 10],
        "num_units": 64,
        "num_layers": 3,
        "num_classes": 5,
        "optimizer": "adam",
        "loss": "sparse_categorical_crossentropy",
        "metrics": ["accuracy"],
        "epochs": 15,
        "batch_size": 64
      }
    ],
    "data": {
      "image_dir": "/workspace/data/images",
      "mask_dir": "/workspace/data/masks",
      "tfrecord_train": "/workspace/data/train.tfrecords",
      "tfrecord_val": "/workspace/data/val.tfrecords",
      "test_size": 0.2
    },
    "gpu": {
      "use_gpu": true,
      "memory_growth": true
    },
    "training": {
      "save_interval": 5,
      "save_dir": "/workspace/models"
    }
  }
  